{
  "name": "netease-music-app",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit --watch --preserveWatchOutput",
    "typecheck:once": "vue-tsc --noEmit",
    "test": "vitest",
    "test:update": "vitest --update",
    "test:run": "vitest --run --changed",
    "benchmark": "vitest bench --changed",
    "analyze": "vite-bundle-visualizer",
    "icon": "pwa-assets-generator",
    "splash": "pwa-asset-generator ./public/splash.html ./public -o false -h -i ./index.html -s false -p \"0\"",
    "compile": "vite build",
    "build": "vue-tsc --noEmit && vite build",
    "format": "eslint --fix --cache src/**/*.{vue,ts}",
    "lint-staged": "lint-staged",
    "preview": "vite preview",
    "prepare": "husky install",
    "release": "commit-and-tag-version --preset=aaron-preset",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ark-ui/vue": "^1.0.0-4",
    "@unhead/vue": "^1.8.8",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.2",
    "eruda": "^3.0.1",
    "fast-average-color": "^9.4.0",
    "lodash": "^4.17.21",
    "paroles": "^2.3.0",
    "pinia": "^2.1.7",
    "spark-md5": "^3.0.2",
    "swiper": "^10.3.1",
    "tippy.js": "^6.3.7",
    "vee-validate": "^4.12.2",
    "vue": "^3.4.15",
    "vue-component-type-helpers": "^1.8.27",
    "vue-route-prefetch": "^3.0.2",
    "vue-router": "^4.2.5",
    "vue-tippy": "^6.3.1",
    "vue-view-transitions": "1.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@iconify-json/fluent": "^1.1.43",
    "@iconify-json/solar": "^1.1.1",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
