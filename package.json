    "vitest": "^1.1.3",
    "vue-tsc": "^1.8.27"
  },
  "pnpm": {
    "patchedDependencies": {
      "@storybook/vue3@7.6.7": "patches/@storybook__vue3@7.6.7.patch",
      "@vue/shared@3.4.15": "patches/@vue__shared@3.4.15.patch"
    }
  }
