    "minimist": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "ora": "^6.3.1",
    "postcss": "^8.4.32",
    "prettier": "^3.2.2",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "proxy-agent": "^6.3.1",
    "pwa-asset-generator": "^6.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.69.5",
    "simple-git": "^3.21.0",
    "storybook": "7.6.7",
